#include <stdio.h>
int plus(int a, int b);
int minus(int a, int b);
int multi(int a, int b);
int div(int a, int b);
int main(){
    int a,b,r;
    scanf("%d %d",&a,&b);
    r=plus(a,b);
    printf("%d\n",r);
    r=minus(a,b);
    printf("%d\n",r);
    r=multi(a,b);
    printf("%d\n",r);
    r=div(a,b);
    printf("%d",r);
    return 0;
}
int plus(int a, int b){
    return a+b;
}
int minus(int a, int b){
    return a-b;
}
int multi(int a, int b){
    return a*b;
}
int div(int a, int b){
    return a/b;
}
